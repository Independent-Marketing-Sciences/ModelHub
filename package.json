{
  "name": "modelling-mate",
  "version": "1.0.7",
  "private": true,
  "author": "Independent Marketing Sciences <info@im-sciences.com>",
  "description": "Professional analytics and modeling platform",
  "homepage": "https://im-sciences.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/Independent-Marketing-Sciences/ModelHub.git"
  },
  "main": "electron/main.js",
  "scripts": {
    "dev": "concurrently \"next dev\" \"npm run python:dev\"",
    "dev:frontend": "next dev",
    "python:dev": "cd backend/src && python main.py",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run dev:frontend\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "next build && electron-builder",
    "electron:build:win": "set CSC_IDENTITY_AUTO_DISCOVERY=false&& next build && electron-builder --win --config electron-builder.yml",
    "electron:publish:win": "set CSC_IDENTITY_AUTO_DISCOVERY=false&& next build && electron-builder --win --config electron-builder.yml --publish always",
    "electron:build:mac": "next build && electron-builder --mac",
    "electron:build:linux": "next build && electron-builder --linux",
    "build:python-backend": "cd backend && python -m PyInstaller main.spec --clean",
    "build:complete": "npm run build:python-backend && npm run electron:build:win",
    "build:bundled": "cd scripts && build-python-backend.bat && cd .. && npm run electron:build:win"
  },
  "build": {
    "appId": "com.imsciences.modellingmate",
    "productName": "Modelling Mate",
    "directories": {
      "output": "dist"
    },
    "files": [
      "electron/**/*",
      ".next/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "electron/icon.ico",
      "sign": null
    },
    "mac": {
      "target": [
        "dmg"
      ],
      "icon": "electron/icon.icns"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "icon": "electron/icon.png"
    },
    "electronDownload": {
      "mirror": "https://npmmirror.com/mirrors/electron/"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.4",
    "@mui/x-date-pickers": "^8.14.1",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-tabs": "^1.1.1",
    "@tanstack/react-table": "^8.20.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.18",
    "electron-updater": "^6.6.2",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.468.0",
    "next": "^15.5.6",
    "next-themes": "^0.4.6",
    "papaparse": "^5.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.14.1",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "xlsx": "^0.18.5",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/papaparse": "^5.3.15",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "cross-env": "^10.1.0",
    "dotenv-cli": "^10.0.0",
    "electron": "^38.3.0",
    "electron-builder": "^25.1.8",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.5.6",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "wait-on": "^8.0.1"
  }
}
